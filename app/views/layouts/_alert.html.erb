<div role="alert" aria-live="assertive" aria-atomic="true" class="toast" data-autohide="true" style="position: fixed;z-index: 3;right: 10px;top: 65px;">
  <div class="toast-header">
    <!-- <img src="..." class="rounded mr-2" alt="..."> -->
    <strong class="mr-auto"><i class="fas fa-exclamation-triangle fa-fw"></i>警告</strong>
    <small>剛剛</small>
    <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="toast-body">
    <%= alert %>
  </div>
</div>
